{
    "name": "sycho/flarum-profile-cover",
    "description": "Adds the ability to add a cover image to a profile.",
    "type": "flarum-extension",
    "license": "MIT",
    "require": {
        "flarum/core": "^0.1.0-beta.10"
    },
    "autoload": {
        "psr-4": {
            "SychO\\ProfileCover\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Pofile Cover",
            "icon": {
                "name": "fas fa-image",
                "backgroundColor": "#004957",
                "color": "#fff"
            }
        }
    }
}
